   <SourceLine>strTekst = ""</SourceLine>
   <SourceLine>If iStartChar &lt; 0 Then iStartChar = 0</SourceLine>
   <SourceLine>If iStopChar &gt; m_cpArr.Ubound Then iStopChar = m_cpArr.Ubound</SourceLine>
   <SourceLine></SourceLine>
