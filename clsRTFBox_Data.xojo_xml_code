   <SourceLine>End If</SourceLine>
   <SourceLine>t = t + 1</SourceLine>
   <SourceLine>Wend</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>System.DebugLog "GetImage.Meten uitgevoerd in " + Format(Round(((Microseconds - dMSTussenStop) / 1000.0)), "0") + " ms"</SourceLine>
