   <SourceLine>bIgnoredPage = False</SourceLine>
   <SourceLine>ElseIf m_cpArr(t - 1).iFunctie &lt;&gt; 0 And GetFunctie(m_cpArr(t - 1).iFunctie) &lt;&gt; "page" Then</SourceLine>
