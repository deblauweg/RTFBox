   <SourceLine>m_cpArr(t).iScreenY = 0</SourceLine>
   <SourceLine>Next t</SourceLine>
   <SourceLine>picMask = Nil</SourceLine>
   <SourceLine>ElseIf bRedrawWithMask Then</SourceLine>
