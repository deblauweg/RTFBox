   <SourceLine>m_cpArr(t).iScreenY = 0</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>Next t</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>iRX = 0</SourceLine>
   <SourceLine>iRY = 0</SourceLine>
